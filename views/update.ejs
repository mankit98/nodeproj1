<%- include('partials/nav') %>
<form class="form m-auto mt-5 w-25  " action="/update/<%=user._id %>" method="post">
    <h1>update page</h1>
    <input class="form-control mb-3" type="text" name="username" value="<%=user.username%>" placeholder="username">
    <input class="form-control mb-3" type="email" name="email" placeholder="john@email.com" value="<%=user.email%>">
    <input class="form-control mb-3" type="password" name="password" value="<%=user.password%>" placeholder="*********" disabled>
    <button class="btn btn-success">update</button>
    <button class="btn btn-primary">reset</button>
   </form>
